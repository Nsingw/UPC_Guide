.buttonPreStart,.buttonPreEnd{
  width: 300rpx; /* 宽度 */
	height: 80rpx; /* 高度 */
  border-width: 0px; /* 边框宽度 */
	border-radius: 50rpx; /* 边框半径 边框扁圆*/
  box-shadow: 2rpx 2rpx 2rpx #888888;/* 边框阴影*/
	outline: none; /* 不显示轮廓线 */
	font-family: Microsoft YaHei; /* 设置字体 */
	color: white; /* 字体颜色 */
	font-size: 34rpx; /* 字体大小 */
  font-weight: bold;
}
.buttonPreStart{
  background: #1E90FF; /* 背景颜色 */
}
.buttonPreEnd{
  background: rgb(216, 33, 13);
}

.buttonPositionExit{
  position: absolute;
  bottom: 55rpx;
  left: 0;/* 居中显示 */
  right: 0;
}
.button{
  width: 540rpx; /* 宽度 */
