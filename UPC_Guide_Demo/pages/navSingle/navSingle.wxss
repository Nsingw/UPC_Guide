.box {
  z-index: 1;
  position: fixed;
    width:100%;
    height: 80rpx;
    /*white-space 不能丢  */
    white-space: nowrap;
    background-color: #0B8BF0;
    box-sizing: border-box
}
.box-item {
    width: 14%;
    height:80%;
    font-size: 30rpx;
    color: white;
    display: inline-block;
     text-align: center;
}
.box-item_checked{
    width: 14%;
    height:80%;
    border-bottom: 6rpx solid white;
    color: white;
    box-sizing: border-box;
    display: inline-block;
    text-align: center;
}

